{
    "name": "swoolecan/baseapp",
    "description": "Hyperf基础组件封装",
    "keywords": [
        "swoolecan",
        "hyperf",
        "baseapp",
        "swoole",
        "user"
    ],
    "homepage": "https://github.com/swoolecan/baseapp",
    "license": "MIT",
    "authors": [
        {
            "name": "swoolecan",
            "homepage": "https://github.com/swoolecan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-swoole": ">=4.5",
        "hyperf/amqp": "~2.0.0",
        "hyperf/async-queue": "~2.0.0",
        "hyperf/cache": "~2.0.0",
        "hyperf/command": "~2.0.0",
        "hyperf/config": "~2.0.0",
        "hyperf/config-aliyun-acm": "~2.0.0",
        "hyperf/constants": "~2.0.0",
        "hyperf/contract": "~2.0.0",
        "hyperf/consul": "^2.0.0",
        "hyperf/database": "~2.0.0",
        "hyperf/db-connection": "~2.0.0",
        "hyperf/di": "~2.0.0",
        "hyperf/dispatcher": "~2.0.0",
        "hyperf/elasticsearch": "~2.0.0",
        "hyperf/event": "~2.0.0",
        "hyperf/exception-handler": "~2.0.0",
        "hyperf/framework": "~2.0.0",
        "hyperf/grpc-client": "~2.0.0",
        "hyperf/grpc-server": "~2.0.0",
        "hyperf/guzzle": "~2.0.0",
        "hyperf/http-server": "~2.0.0",
        "hyperf/json-rpc": "^2.0",
        "hyperf/logger": "~2.0.0",
        "hyperf/memory": "~2.0.0",
        "hyperf/model-cache": "~2.0.0",
        "hyperf/paginator": "~2.0.0",
        "hyperf/pool": "~2.0.0",
        "hyperf/process": "~2.0.0",
        "hyperf/redis": "~2.0.0",
        "hyperf/rpc-server": "^2.0",
        "hyperf/service-governance": "~2.0.0",
        "hyperf/tracer": "~2.0.0",
        "hyperf/translation": "^2.0",
        "hyperf/utils": "~2.0.0",
        "hyperf/validation": "^2.0",
        "league/fractal": "^0.19.2",
        "overtrue/easy-sms": "^1.1",
        "swoolecan/hyperf-resource": "dev-master"
    },
    "require-dev": {
        "hyperf/testing": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Swoolecan\\Baseapp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swoolecan\\Baseapp\\Test\\": "tests"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Swoole\\Baseapp\\ConfigProvider"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
