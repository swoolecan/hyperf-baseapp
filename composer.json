{
    "name": "swoolecan/baseapp",
    "description": "Hyperf基础组件封装",
    "keywords": [
        "swoolecan",
        "hyperf",
        "baseapp",
        "swoole",
        "user"
    ],
    "homepage": "https://github.com/swoolecan/baseapp",
    "license": "MIT",
    "authors": [
        {
            "name": "swoolecan",
            "homepage": "https://github.com/swoolecan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-swoole": ">=4.4",
        "hyperf/cache": "~1.1.0",
        "hyperf/command": "~1.1.0",
        "hyperf/config": "~1.1.0",
        "hyperf/db-connection": "~1.1.0",
        "hyperf/framework": "~1.1.0",
        "hyperf/guzzle": "~1.1.0",
        "hyperf/http-server": "~1.1.0",
        "hyperf/logger": "~1.1.0",
        "hyperf/memory": "~1.1.0",
        "hyperf/process": "~1.1.0",
        "hyperf/redis": "~1.1.0",
        "hyperf/grpc-client": "~1.1.0",
        "hyperf/grpc-server": "~1.1.0",
        "hyperf/config-aliyun-acm": "~1.1.0",
        "hyperf/constants": "~1.1.0",
        "hyperf/async-queue": "^1.1",
        "hyperf/amqp": "~1.1.0",
        "hyperf/model-cache": "~1.1.0",
        "hyperf/elasticsearch": "~1.1.0",
        "hyperf/tracer": "~1.1.0",

        "hyperf/contract": "~1.1.0",
        "hyperf/database": "~1.1.0",
        "hyperf/di": "~1.1.0",
        "hyperf/dispatcher": "~1.1.0",
        "hyperf/event": "~1.1.0",
        "hyperf/exception-handler": "~1.1.0",
        "hyperf/paginator": "~1.1.0",
        "hyperf/pool": "~1.1.0",,
        "hyperf/utils": "~1.1.22",
        "hyperf/validation": "^1.1",
        "hyperf/translation": "^1.1",
        "hyperf/json-rpc": "^1.1",
        "hyperf/rpc-server": "^1.1",
        "overtrue/easy-sms": "^1.1"
    },
    "require-dev": {
        "hyperf/testing": "~1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Swoolecan\\Baseapp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swoolecan\\Baseapp\\Test\\": "tests"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Swoole\\Baseapp\\ConfigProvider"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
